-- Create table
create table LPV_INTERFAZ_WS.WBS_MAPS
(
  ID         NUMBER not null,
  NAME       NVARCHAR2(255),
  INTERFAZ   NVARCHAR2(255),
  INPUT      NVARCHAR2(255),
  OUTPUT     NVARCHAR2(255),
  DETAIL     NVARCHAR2(255),
  LASTCHANGE DATE
)
tablespace Z00
  pctfree 10
  initrans 1
  maxtrans 255
  storage
  (
    initial 64K
    next 1M
    minextents 1
    maxextents unlimited
  );
-- Create/Recreate primary, unique and foreign key constraints 
alter table LPV_INTERFAZ_WS.WBS_MAPS
  add constraint PK_MAPS primary key (ID)
  using index 
  tablespace Z00
  pctfree 10
  initrans 2
  maxtrans 255
  storage
  (
    initial 64K
    next 1M
    minextents 1
    maxextents unlimited
  );
-- Create/Recreate indexes 
create index LPV_INTERFAZ_WS.IDX_NAME on LPV_INTERFAZ_WS.WBS_MAPS (NAME, INTERFAZ, INPUT)
  tablespace Z00
  pctfree 10
  initrans 2
  maxtrans 255
  storage
  (
    initial 64K
    next 8K
    minextents 1
    maxextents unlimited
  );
